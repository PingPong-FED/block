{
  "name": "@pp/block",
  "version": "1.0.0",
  "description": "专为 Axios.js 开发的模块API管理模式。它采用集中式存储管理应用的所有 API 模块，并以相应的规则保证 API 以一种统一的方式进行调用。现已支持Vuex Plugin",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clear": "rm -rf dist",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:publish": "npm run docs:build && def-deploy --path ./docs/dist",
    "build": "npm run clear && tsc",
    "watch": "tsc -w",
    "test": "jest --forceExit --coverage --verbose",
    "test:watch": "npm run test -- --watchAll",
    "commit": "npm run build && npm run docs:publish && yarn publish"
  },
  "author": "suliyu",
  "license": "MIT",
  "dependencies": {
    "@types/jest": "^23.1.3",
    "@types/node": "^10.5.1",
    "@types/qs": "^6.5.1",
    "@types/url-template": "^2.0.28",
    "axios": "^0.18.0",
    "qs": "^6.5.2",
    "reattempt": "^0.1.0",
    "tslib": "^1.9.3",
    "tslint-config-standard": "^7.1.0",
    "url-template": "^2.0.8",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "build": "^0.1.4",
    "jest": "^23.2.0",
    "ts-jest": "^23.0.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "vuepress": "^0.14.2"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
}
